		<class>edu.neu.cs5200.finalproj.model.KStudygroup</class>
		<class>edu.neu.cs5200.finalproj.model.KUser_Studygroup</class>
		<class>edu.neu.cs5200.finalproj.model.KUser</class>
		
		<properties>
			<property name="openjpa.ConnectionUserName"
				value="kickdata"/>
			<property name="openjpa.ConnectionPassword"
				value="datastarter2015"/>
			<property name="openjpa.ConnectionURL"
				value="jdbc:mysql://localhost:3306/kickdatastarter"/>
